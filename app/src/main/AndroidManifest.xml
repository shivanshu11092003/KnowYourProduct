<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/black___blue_minimalist_modern_initial_font_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/black___blue_minimalist_modern_initial_font_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.KnowYourProduct"
        tools:targetApi="34">
        <activity
            android:name=".Activity.Slider_item"
            android:exported="false" />
        <activity
            android:name=".Activity.eachpost"
            android:exported="false" />
        <activity
            android:name=".Activity.Login"
            android:exported="true">

        </activity>

        <activity
            android:name=".Activity.SplashScreen"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>